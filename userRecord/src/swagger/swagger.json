{
    "swagger": "2.0",
    "info": {
      "version": "V1",
      "title": "Google Keep",
      "description": "This is the replica of Google Keep. Users can register, login, and perform CRUD operations on notes."
    },
    "host": "localhost:3000",
    "basePath": "/api/v1/",
    "tags": [
        {
            "name": "Google keep using express CLI",
            "description": "This is the replica of google keep"
          }
    ],
    "schemes": ["http:localhost:27017/mydb"],
    "consumes": ["application/json"],
    "produces": ["application/json"],
    "paths": {
      "/users": {
        "post": {
          "tags": ["User"],
          "summary": "Register a new user",
          "description": "Register a new user.",
          "parameters": [
            {
              "name": "User",
              "in": "body",
              "description": "User object",
              "required": true,
              "schema": {
                "$ref": "#/definitions/User"
              }
            }
          ],
          "responses": {
            "201": {
              "description": "User created successfully"
            },
            "500": {
              "description": "Internal server error"
            }
          }
        }
      },
      "/users/login": {
        "post": {
          "tags": ["User"],
          "summary": "User login",
          "description": "Authenticate user credentials.",
          "parameters": [
            {
              "name": "Credentials",
              "in": "body",
              "description": "User credentials",
              "required": true,
              "schema": {
                "$ref": "#/definitions/Login"
              }
            }
          ],
          "responses": {
            "201": {
              "description": "Successfully logged in"
            },
            "404": {
              "description": "Email or password doesn't match"
            },
            "500": {
              "description": "Internal server error"
            }
          }
        }
      },
      "/notes": {
        "post": {
          "tags": ["Notes"],
          "summary": "Create a new note",
          "description": "Create a new note for the user.",
          "parameters": [
            {
              "name": "Note",
              "in": "body",
              "description": "Note object",
              "required": true,
              "schema": {
                "$ref": "#/definitions/Note"
              }
            }
          ],
          "responses": {
            "201": {
              "description": "Note created successfully"
            },
            "500": {
              "description": "Internal server error"
            }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        },
      
        "get": {
          "tags": ["Notes"],
          "summary": "Get all notes",
          "description": "Retrieve all notes for the user.",
          "responses": {
            "200": {
              "description": "Notes fetched successfully"
            },
            "500": {
              "description": "Internal server error"
            }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        }
      },
    
      "/notes/{noteid}": {
        "get": {
          "tags": ["Notes"],
          "summary": "Get a single note by ID",
          "description": "Retrieve a single note by its ID.",
          "parameters": [
            {
              "name": "noteid",
              "in": "path",
              "description": "ID of the note to retrieve",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "Note fetched successfully"
            },
            "500": {
              "description": "Internal server error"
            }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        },
        "put": {
          "tags": ["Notes"],
          "summary": "Update a note",
          "description": "Update an existing note.",
          "parameters": [
            {
              "name": "noteid",
              "in": "path",
              "description": "ID of the note to update",
              "required": true,
              "type": "string"
            },
            {
              "name": "Note",
              "in": "body",
              "description": "Updated note object",
              "required": true,
              "schema": {
                "$ref": "#/definitions/Note"
              }
            }
          ],
          "responses": {
            "202": {
              "description": "Note updated successfully"
            },
            "500": {
              "description": "Internal server error"
            }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        },
        "delete": {
          "tags": ["Notes"],
          "summary": "Delete a note",
          "description": "Delete an existing note.",
          "parameters": [
            {
              "name": "noteid",
              "in": "path",
              "description": "ID of the note to delete",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "Note deleted successfully"
            },
            "500": {
              "description": "Internal server error"
            }
          },
          "security": [
            {
              "JWT": []
            }
          ]
        }
      }
    },
    "securityDefinitions": {
      "JWT": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    },
    "definitions": {
      "User": {
        "type": "object",
        "properties": {
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "Login": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "Note": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
         
         
        }
      }
    }
  }
  